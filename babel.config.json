{
  "presets": [
    [
      "minify",
      {
        "builtIns": false
      }
    ],
    [
      "@babel/preset-env",
      {
        "modules": "auto",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "vant",
        "libraryDirectory": "es",
        "style": true
      },
      "vant"
    ],
    "@babel/plugin-transform-typescript",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-optional-chaining"
  ],
  "babelrcRoots": ["."],
  "overrides": [
    {
      "ignore": [
        "**/node_modules/*",
        "**/vendor/*"
      ]
    }
  ]
}
